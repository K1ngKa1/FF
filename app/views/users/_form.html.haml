= form_for @user do |f|
  .form-group
    = f.label "Business Type"
    = f.select(:business_type, options_for_select(['LLC','S Corp', 'C Corp']))
  .form-group
    = f.label "City"
    = f.text_field :city
  .form-group
    = f.label "State"
    = f.text_field :state
  .form-group
    = f.label "Task"
    = f.select(:task, options_for_select(['Move','Taxes']))
  = f.submit "Find Forms"


